            <Image x:Name="second" HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="60" Margin="617,47,0,0"  Source="/RLC_Analysis;component/images/Empty2.png" AllowDrop="True" Drop="Image_Drop"/>
